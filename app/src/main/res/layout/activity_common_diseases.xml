<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cut">

    <TextView
        android:id="@+id/textView41112"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Common Disease"
        android:textColor="#040303"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/homeBtnCommonDisease"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="sans-serif-black"
        android:text="Home"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/homeBtnCommonDisease"
        app:layout_constraintTop_toBottomOf="@+id/textView41112"
        tools:layout_editor_absoluteX="207dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingStart="10dp"
            android:paddingEnd="10dp">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:text="Alpha-1-Antritrypsin deficiency (Emphysema) Anemia Appendicitis -atypical Arthritis Asthma Bacteria Blood clots (Embolism) Cancer Cholelithiasis (Gallstones) Diabetes Diabetic Retinopathy Embolism Emphysema Eye Terms  Common Eye Problems Gallbladder disease Gallstones Gastroesophageal reflux disease – GERD Heart (Atrial Fibrillation) Heberden’s nodes (Arthritis) Hyperlipoproteinemia Hypertension Influenza Lyme Disease Review Liver disease Osteoporosis Prions Skin diseases Staph Infections [MRSA] Stenosis Stroke Vertigo Viruses (Prions)"
                android:textColor="#090909"
                android:textSize="30sp"
                android:textStyle="bold" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>